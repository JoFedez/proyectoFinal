{
    "name": "johan9mx9tzb/cv_generator",
    "description": "Una aplicaci√≥n MVC para generar CVs en PDF.",
    "type": "project",
    "require": {
        "dompdf/dompdf": "^2.0",
        "tecnickcom/tcpdf": "^6.7",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require-dev": {
        "laravel/breeze": "^2.2"
    }
}
